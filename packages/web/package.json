{
    "name": "packages-web",
    "private": true,
    "type": "module",
    "scripts": {
        "postinstall": "nuxt prepare",
        "build": "nuxt build",
        "dev": "nuxt dev",
        "serve": "PORT=$NODE_PORT HOST=$NODE_HOST bun run $APP_SERVER",
        "lint": "bun run lint:eslint && bun run lint:knip",
