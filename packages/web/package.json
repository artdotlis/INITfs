        "profile": "PORT=$NODE_PORT HOST=$NODE_HOST nuxt analyze"
    },
    "dependencies": {
        "@nuxt/image": "^1.9",
        "nuxt-security": "^2.2",
        "nuxt": "^3.16",
        "vue": "^3.5",
        "vue-router": "^4.5"
    },
    "devDependencies": {
        "@nuxt/eslint": "^1.2",
        "@eslint/js": "^9.22",
        "@typescript-eslint/parser": "^8.26",
        "eslint-import-resolver-typescript": "^4.2",
        "eslint-plugin-import-x": "^4.8",
        "knip": "^5.46",
        "vite": "^6.2"
    }
}
